    <meta name="viewport" content="width=device-width, initial-scale=1.0">
		<link rel="stylesheet" href="/assets/foundation-icons/foundation-icons.css" />
		<link rel="stylesheet" href="/assets/bundle.css">
    <title>{{ title }}</title>

		<!-- Meta -->
		<meta name="author" content="Carlos González">
    <meta name="keywords" content="rastrexo,rastrexar,rastrexando,andaina,galicia"/>

		<!-- Favicon (made with http://realfavicongenerator.net/) -->
    <link rel="apple-touch-icon" sizes="180x180" href="/assets/apple-touch-icon.png?v2=Gvk3Pm9dNO">
    <link rel="icon" type="image/png" sizes="32x32" href="/assets/favicon-32x32.png?v2=Gvk3Pm9dNO">
    <link rel="icon" type="image/png" sizes="16x16" href="/assets/favicon-16x16.png?v2=Gvk3Pm9dNO">
    <link rel="manifest" href="/assets/site.webmanifest?v2=Gvk3Pm9dNO">
    <link rel="mask-icon" href="/assets/safari-pinned-tab.svg?v2=Gvk3Pm9dNO" color="#5bbad5">
    <link rel="shortcut icon" href="/assets/favicon.ico?v2=Gvk3Pm9dNO">
    <meta name="apple-mobile-web-app-title" content="Rastrexando">
    <meta name="application-name" content="Rastrexando">
    <meta name="msapplication-TileColor" content="#da532c">
    <meta name="theme-color" content="#ffffff">
  </head>
  <body>
		<div id="layout-base">
			<header id="main-header">
				<div class="wrapper">
					<a id="rastrexando-logo" href="/">
						<img class="image" src="/assets/rastrexando-circular.png" />
						<span class="title">Rastrexando</span>
					</a>
					{% if header_icon %}
						<a href="{{ header_link }}" title="{{ header_title }}" class="link header-icon"><i class="{{ header_icon }}"></i></a>
					{% endif %}
				</div>
			</header>
			<main>
				<div class="page-content">
					{{ content | safe }}
				</div>
			</main>
			<footer id="main-footer">
				<div class="wrapper">
					<div class="left">
						<p>Rastrexando</p>
						©
						<p>2017-2024</p>
					</div>
					<div class="right">
						<i class="fi-mail"></i>
						<a href="mailto:rastrexando.eu@gmail.com">Contacto</a>
					</div>
				</div>
			</footer>
		</div>
